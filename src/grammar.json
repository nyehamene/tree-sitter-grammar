{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "grammar",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "declaration"
      }
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "binding"
        },
        {
          "type": "SYMBOL",
          "name": "rule"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "binding": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "import_directive"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "production"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "production": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "production"
              },
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "sequence"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "sequence"
          }
        ]
      }
    },
    "sequence": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_basic"
      }
    },
    "_basic": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "repetition"
        },
        {
          "type": "SYMBOL",
          "name": "optional"
        },
        {
          "type": "SYMBOL",
          "name": "_term"
        }
      ]
    },
    "group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "production"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "repetition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "production"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "optional": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "production"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_terminal"
        },
        {
          "type": "SYMBOL",
          "name": "_non_terminal"
        }
      ]
    },
    "_non_terminal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "member_access"
        }
      ]
    },
    "_terminal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "regexp"
        }
      ]
    },
    "member_access": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "object",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "ident"
                },
                {
                  "type": "SYMBOL",
                  "name": "member_access"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "FIELD",
            "name": "property",
            "content": {
              "type": "SYMBOL",
              "name": "ident"
            }
          }
        ]
      }
    },
    "import_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@import"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "path",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "[^\"\\n]*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "ident": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_][a-zA-Z0-9_]*"
      }
    },
    "regexp": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/"
          },
          {
            "type": "PATTERN",
            "value": "[^\\/]*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [
    "_basic",
    "_term",
    "_non_terminal",
    "_terminal"
  ],
  "supertypes": [],
  "reserved": {}
}